{
  "name": "sos-bridge-frontend",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "turbo dev",
    "build": "turbo build",
    "lint": "turbo lint",
    "dev:mini-app": "turbo dev --filter=mini-app",
    "dev:dashboard": "turbo dev --filter=dashboard",
    "build:mini-app": "turbo build --filter=mini-app",
    "build:dashboard": "turbo build --filter=dashboard"
  },
  "devDependencies": {
    "turbo": "^2.3.0",
    "typescript": "^5.7.2"
  },
  "packageManager": "npm@10.2.0",
  "engines": {
    "node": ">=18"
  }
}








